<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcome-file>/jsp/login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>LoginController</servlet-name>
    <servlet-class>com.hqyj.servletDemo.controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginController</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DoLoginController</servlet-name>
    <servlet-class>com.hqyj.servletDemo.controller.DoLoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DoLoginController</servlet-name>
    <url-pattern>/doLogin</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RegisterController</servlet-name>
    <servlet-class>com.hqyj.servletDemo.controller.RegisterController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterController</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DoRegisterController</servlet-name>
    <servlet-class>com.hqyj.servletDemo.controller.DoRegisterController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DoRegisterController</servlet-name>
    <url-pattern>/doRegister</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>IndexController</servlet-name>
    <servlet-class>com.hqyj.servletDemo.controller.IndexController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>IndexController</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UsersController</servlet-name>
    <servlet-class>com.hqyj.servletDemo.controller.UsersController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UsersController</servlet-name>
    <url-pattern>/users</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UpdateUserController</servlet-name>
    <servlet-class>com.hqyj.servletDemo.controller.UpdateUserController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UpdateUserController</servlet-name>
    <url-pattern>/updateUser</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DoUpdateUserController</servlet-name>
    <servlet-class>com.hqyj.servletDemo.controller.DoUpdateUserController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DoUpdateUserController</servlet-name>
    <url-pattern>/doUpdateUser</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DeleteUserController</servlet-name>
    <servlet-class>com.hqyj.servletDemo.controller.DeleteUserController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteUserController</servlet-name>
    <url-pattern>/deleteUser</url-pattern>
  </servlet-mapping>
</web-app>